<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#FF7344" />
            <stroke
                android:width="1dp"
                android:color="#FFFFFF" />
            <corners
                android:radius="30dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="@color/bgSecondary" />
            <stroke
                android:width="1dp"
                android:color="@color/bgPrimary" />

        </shape>
    </item>
</selector>